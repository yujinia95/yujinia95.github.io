{
    "routes": [
      {
        "route": "/api/*",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    }
  }